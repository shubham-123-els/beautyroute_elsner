<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="add_product_to_sync_queue">
                <settings>
                    <confirm>
                        <message translate="true">Add product(s) to the sync queue?</message>
                        <title translate="true">Add to sync queue</title>
                    </confirm>
                    <url path="qbonline/queue/item"/> <!-- controller  -->
                    <type>add to sync queue</type>
                    <label translate="true">Add to sync queue</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
